fun Vector(x, y) {
    fun vec(method) {
        if (method == "getX") {
            return x;
        }
        if (method == "getY") {
            return y;
        }
        if (method == "print") {
            print "Vector";
            print x;
            print y;
            return;
        }
    }

    return vec;
}

fun addVecs(v1, v2) {
    return Vector(v1("getX") + v2("getX"), v1("getY") + v2("getY"));
}

var v1 = Vector(1, 2);
var v2 = Vector(2, 3);

var v3 = addVecs(v1, v2);

